
func TestPointer(t *testing.T) {

	type input struct {
		input *string
	}
	type expected struct {
		gotPstring *string
	}
	tests := []struct {
		Name     string
		Input    input
		Expected expected
	}{
		// TODO: Add test cases.
	}

	for _, test := range tests {
		t.Run(test.Name, func(t *testing.T) {

			gotPstring := pointer(test.Input.input)

			assert.Equal(t, test.Expected.gotPstring, gotPstring)
		})
	}
}